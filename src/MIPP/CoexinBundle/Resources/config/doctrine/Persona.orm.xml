    <field name="codArea" type="string" column="cod_area" nullable="true"/>
    <field name="telf1" type="string" column="telf1" nullable="true"/>
    <field name="telf2" type="string" column="telf2" nullable="true"/>
    <field name="telf3" type="string" column="telf3" nullable="true"/>
    <field name="fax" type="string" column="fax" nullable="true"/>
    <field name="lugarResidencia" type="string" column="lugar_residencia" nullable="true"/>
    <field name="numerosDestino" type="string" column="numeros_destino" nullable="true"/>
