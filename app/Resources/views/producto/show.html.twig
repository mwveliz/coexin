{% extends 'base.html.twig' %}

{% block body %}
    <h1>Producto</h1>

    

     
         
        <table class="table table-striped table-bordered table-hover dt-responsive" width="70%" id="sample_2">
<tbody>
     
         
            <tr>
                <th>Denominacioncomercial</th>
                <td>{{ producto.denominacionComercial }}</td>
            </tr>
            <tr>
                <th>Unidadmedida</th>
                <td>{{ producto.unidadMedida }}</td>
            </tr>
            <tr>
                <th>Procesoproductivoliteral</th>
                <td>{{ producto.procesoProductivoLiteral }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{% if producto.isActive %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                
              
            </tr>
        </tbody>
   </table>

     
         {{ form_start(delete_form) }}

     
         
            <a href="{{ path('producto_index') }}" class="btn btn-warning" role="button">Cancelar</a>
         
         
            <a href="{{ path('producto_edit', { 'id': producto.id }) }}" class="btn btn-info" role="button">Editar</a>
         
         
            
                <button type="submit" class="btn btn-danger">Eliminar</button>
            {{ form_end(delete_form) }}
         
     
{% endblock %}
